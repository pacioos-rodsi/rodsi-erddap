<dataset type="EDDTableFromAsciiFiles" datasetID="lahaina_chlorophyll_samples" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/datasets/maui_fires/samples/</fileDir>
    <fileNameRegex>lahaina_chlorophyll_events\.csv</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <charset>ISO-8859-1</charset>
    <columnSeparator></columnSeparator>
    <columnNamesRow>1</columnNamesRow>
    <firstDataRow>2</firstDataRow>
    <sortedColumnSourceName>HST</sortedColumnSourceName>
    <sortFilesBySourceNames>HST</sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <!-- sourceAttributes>
    </sourceAttributes -->
    <!-- Please specify the actual cdm_data_type (TimeSeries?) and related info below, for example...
        <att name="cdm_timeseries_variables">station_id, longitude, latitude</att>
        <att name="subsetVariables">station_id, longitude, latitude</att>
    -->
    <addAttributes>
        <att name="cdm_data_type">Point</att>
        <att name="Conventions">COARDS, CF-1.10, ACDD-1.3</att>
        <att name="creator_name">University of Hawaii at Manoa</att>
        <att name="creator_type">institution</att>
        <att name="infoUrl">???</att>
        <att name="institution">University of Hawaii at Manoa</att>
        <att name="keywords">chlorophyll, data, fires, hawaii, hst, identifier, latitude, longitude, manoa, maui, response, sample, SAMPLE_ID, samples, sensor, site, time, university, unix, value</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="summary">Maui Fires Response - chlorophyll samples</att>
        <att name="title">Maui Fires Response, chlorophyll samples</att>
    </addAttributes>
    <dataVariable>
        <sourceName>SAMPLE ID</sourceName>
        <destinationName>SAMPLE_ID</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Identifier</att>
            <att name="long_name">SAMPLE ID</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>SAMPLE</sourceName>
        <destinationName>SAMPLE</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">SAMPLE</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>SITE</sourceName>
        <destinationName>SITE</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">SITE</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>SENSOR</sourceName>
        <destinationName>SENSOR</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">SENSOR</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>LAT</sourceName>
        <destinationName>latitude</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">90.0</att>
            <att name="colorBarMinimum" type="double">-90.0</att>
            <att name="ioos_category">Location</att>
            <att name="long_name">Latitude</att>
            <att name="standard_name">latitude</att>
            <att name="units">degrees_north</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>LONG</sourceName>
        <destinationName>longitude</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">180.0</att>
            <att name="colorBarMinimum" type="double">-180.0</att>
            <att name="ioos_category">Location</att>
            <att name="long_name">Longitude</att>
            <att name="source_name">LONG</att>
            <att name="standard_name">longitude</att>
            <att name="units">degrees_east</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>HST</sourceName>
        <destinationName>time</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Time</att>
            <att name="long_name">HST</att>
            <att name="source_name">HST</att>
            <att name="standard_name">time</att>
            <att name="time_precision">1970-01-01T00:00Z</att>
            <att name="units">M/d/yyyy H:m</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>VALUE</sourceName>
        <destinationName>VALUE</destinationName>
        <dataType>float</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="float">NaN</att>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">VALUE</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>UNIX</sourceName>
        <destinationName>UNIX</destinationName>
        <dataType>int</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="int">2147483647</att>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">UNIX</att>
        </addAttributes>
    </dataVariable>
</dataset>
